<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MujerAtras"
    android:background="@drawable/hombreatras">

    <!-- TODO: poner todo 22 pd mas abajo-->
    <ImageButton
        android:id="@+id/buttonEspalda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/hespalda"
        android:onClick="onClickEspalda"
        android:background="@null"
        android:layout_marginTop="141dp"
        android:layout_below="@+id/buttonAtras"
        android:layout_alignRight="@+id/buttonGluteo"
        android:layout_alignEnd="@+id/buttonGluteo" />

    <!-- FIXME: android:layout_marginTop="154dp 177"-->




    <ImageButton
        android:id="@+id/buttonGluteo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        android:onClick="onClickGluteo"
        android:src="@drawable/hgluteos"
        android:layout_below="@+id/buttonEspalda"
        android:layout_centerHorizontal="true" />

    <!-- FIXME: android:layout_marginTop="155dp 159"-->




    <ImageButton
        android:id="@+id/buttonGirar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        android:onClick="onClickGirar"
        android:src="@drawable/flecha"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Atras"
        android:id="@+id/buttonAtras"
        android:onClick="onClickAtras"
        android:layout_marginTop="100dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>
